- targets_config:
    docker_sd_configs:
      - host: unix:///var/run/docker.sock
        filters:
          - name: label
            values: ["monitoring.enable=true"]
    relabel_configs:
      - source_labels: [__meta_docker_container_label_monitoring_port]
        target_label: __metrics_path__
      - source_labels: [__meta_docker_container_label_monitoring_scheme]
        target_label: __scheme__
